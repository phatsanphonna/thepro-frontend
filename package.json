{
  "name": "thepro-frontend",
  "version": "0.1.0",
  "private": true,
  "target": "esnext",
  "module": "commonjs",
  "scripts": {
    "dev": "cp ./next.config.dev.js ./next.config.js && next dev -p 5001",
    "build": "cp ./next.config.prod.js ./next.config.js && next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "build:image": "docker build -t asia.gcr.io/the-pro-tutor-by-kppm/thepro-frontend ./",
    "deploy": "docker push asia.gcr.io/the-pro-tutor-by-kppm/thepro-frontend"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "@next/bundle-analyzer": "^12.1.6",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "formik": "^2.2.9",
    "framer-motion": "^6.2.9",
    "next": "^12.3.1",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "react-player": "^2.12.0",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "sharp": "^0.30.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/cors": "^2.8.12",
    "@types/node": "17.0.23",
    "@types/react": "^18.0.15",
    "@types/react-dom": "17.0.14",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.3",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
